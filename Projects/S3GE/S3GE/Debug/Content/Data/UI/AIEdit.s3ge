            <gizmo name="AIEditObjectNavmeshGeometrySingleFile" parent="AIEditObjectNavmeshGeometrySingle" description="*.Obj File Import">
              <orientate ref="AIEditObjectNavmeshGeometrySingle" width="256" height="24" xalign="right">
                <physics noresponse="true"/>
              </orientate>
              <variable name="toggle" value="0"/>
              <variable name="linecount" value="1"/>
              <variable name="1" value="Content/3D/dungeon.obj"/>
              <event state="IDLE" behavior="textbox" transition="ghostly" theme="citrusbox" style="standard" scheme="pointingFX" x="24" xalign="left" yalign="middle"/>
              <event state="ENTER"/>
              <event state="HOVER" theme="notepadder" style="textboxtypeset" scheme="pointingFX"/>
              <event state="DOWN" theme="notepadder" style="textboxtypeset"/>
              <event state="HOLD" theme="notepadder" style="textboxtypeset"/>
              <event state="DRAG" theme="notepadder" style="textboxtypeset"/>
              <event state="UP"/>
              <event state="EXIT"/>
            </gizmo>

            <gizmo name="AIEditObjectNavmeshGeometrySingleFileImport" parent="AIEditObjectNavmeshGeometrySingle" description="Import!!!">
              <orientate ref="AIEditObjectNavmeshGeometrySingleFile" width="48" height="24" xalign="right">
                <physics noresponse="true"/>
              </orientate>
              <variable name="toggle" value="0"/>
              <event state="IDLE" behavior="pushbox" transition="ghostly" theme="citrusbox" style="standard" scheme="pointingFX" label="Import" x="24" xalign="left" yalign="middle"/>
              <event state="ENTER"/>
              <event state="HOVER" scheme="pointingFX"/>
              <event state="DOWN"/>
              <event state="HOLD"/>
              <event state="DRAG"/>
              <event state="UP">
                <script language="darklua" type="1">
                  <![CDATA[
                           aiNavGeometryLoad(uiGizmoPropertyGet(uiGizmoGet('AIEditObjectNavmeshGeometrySingleFile'),'Caption'))
				            ]]>
                </script>
              </event>
              <event state="EXIT"/>
            </gizmo>

