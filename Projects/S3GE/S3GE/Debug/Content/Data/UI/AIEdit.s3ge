<s3ge version="1.0">

  <!-- AIEdit Sub Menu-->

  <gizmo name="AIEditFile" parent="AIEdit" description="AI File">
    <variable name="toggle" value="0"/>
    <orientate ref="S3GEEdit" x="2" y="49" width="64" height="24">
      <physics noresponse="true"/>
    </orientate>
    <event state="IDLE" behavior="menubox" transition="ghostly" theme="citrusbox" style="standard" scheme="pointingFX" label="File" xalign="center" yalign="middle"/>
    <event state="ENTER"/>
    <event state="HOVER" scheme="pointingFX"/>
    <event state="DOWN"/>
    <event state="HOLD"/>
    <event state="DRAG"/>
    <event state="UP"/>
    <event state="EXIT"/>
  </gizmo>

    <gizmo name="AIEditFileNew" parent="AIEditFile" ref="previous" description="Create AI File">
