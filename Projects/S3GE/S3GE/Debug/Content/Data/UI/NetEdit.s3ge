      </orientate>
      <event state="IDLE" label="Paste" x="24" xalign="left" yalign="middle"/>
    </gizmo>

    <gizmo name="NetEditEditDelete" parent="NetEditEdit" ref="previous" description="Delete|Clear">
      <orientate ref="previous" width="96" height="24" yalign="bottom">
        <physics noresponse="true"/>
      </orientate>
      <event state="IDLE" label="Delete" x="24" xalign="left" yalign="middle"/>
    </gizmo>

  <gizmo name="NetEditObject" parent="NetEdit" ref="previous" description="Network Objects">
    <orientate ref="previous" width="64" height="24" xalign="right">
      <physics noresponse="true"/>
    </orientate>
    <event state="IDLE" label="Objects" xalign="center" yalign="middle"/>
  </gizmo>

    <gizmo name="NetEditObjectServer" parent="NetEditObject" ref="previous" description="Server">
      <orientate ref="previous" width="96" height="24" yalign="bottom">
        <physics noresponse="true"/>
      </orientate>
      <event state="IDLE" label="Server" x="24" xalign="left" yalign="middle"/>
    </gizmo>
  
    <gizmo name="NetEditObjectHost" parent="NetEditObject" ref="previous" description="Host">
