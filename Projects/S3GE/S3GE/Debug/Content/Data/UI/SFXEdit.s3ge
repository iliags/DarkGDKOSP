  <gizmo name="SFXEditEditDelete" parent="SFXEditEdit" ref="previous" description="Delete|Clear">
      <orientate ref="previous" width="96" height="24" yalign="bottom">
        <physics noresponse="true"/>
      </orientate>
      <event state="IDLE" label="Delete" x="24" xalign="left" yalign="middle"/>
    </gizmo>

  <gizmo name="SFXEditObject" parent="SFXEdit" ref="SFXEditEdit" description="Audio Objects">
    <orientate ref="SFXEditEdit" width="64" height="24" xalign="right">
    <physics noresponse="true"/>
    </orientate>
    <event state="IDLE" label="Objects" xalign="center" yalign="middle"/>
  </gizmo>

