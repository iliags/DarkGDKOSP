<s3ge version="1.0">

 <!-- SFXEdit Sub Menu-->

  <gizmo name="SFXEditFile" parent="SFXEdit" description="Audio Files">
    <variable name="toggle" value="0"/>
    <orientate ref="S3GEEdit" x="2" y="49" width="64" height="24">
      <physics noresponse="true"/>
    </orientate>
    <event state="IDLE"  behavior="menubox" transition="ghostly" theme="waterbox" style="standard" scheme="pointingFX" caption="File" xalign="center" yalign="middle"/>
    <event state="ENTER"/>
    <event state="HOVER" scheme="pointingFX"/>
    <event state="DOWN"/>
    <event state="HOLD"/>
    <event state="DRAG"/>
    <event state="UP"/>
    <event state="EXIT"/>
  </gizmo>

    <gizmo name="SFXEditFileNew" parent="SFXEditFile" ref="previous" description="New Audio File">
      <orientate ref="previous" width="96" height="24" yalign="bottom">
        <physics noresponse="true"/>
      </orientate>
      <event state="IDLE" label="New" x="24" xalign="left" yalign="middle"/>
    </gizmo>

    <gizmo name="SFXEditFileOpen" parent="SFXEditFile" ref="previous" description="Open|Open ReadOnly|Open Recent|Re-Open(Revert) Audio Format(*.s3ge) | Import">
      <orientate ref="previous" width="96" height="24" yalign="bottom">
        <physics noresponse="true"/>
      </orientate>
      <event state="IDLE" label="Open" x="24" xalign="left" yalign="middle"/>
    </gizmo>

    <gizmo name="SFXEditFileSave" parent="SFXEditFile" ref="previous" description="Save|SaveAs Audio Format(*.s3ge)">
      <orientate ref="previous" width="96" height="24" yalign="bottom">
        <physics noresponse="true"/>
      </orientate>
      <event state="IDLE"  label="Save" x="24" xalign="left" yalign="middle"/>
    </gizmo>

    <gizmo name="SFXEditFileClose" parent="SFXEditFile" ref="previous" description="Close|CloseAll|CloseAllBut">
      <orientate ref="previous" width="96" height="24" yalign="bottom">
        <physics noresponse="true"/>
      </orientate>
      <event state="IDLE"  label="Close" x="24" xalign="left" yalign="middle"/>
    </gizmo>

 <gizmo name="SFXEditEdit" parent="SFXEdit" ref="SFXEditFile" description="Audio Edit Oprions">
    <variable name="toggle" value="0"/>
    <orientate ref="SFXEditFile" width="64" height="24" xalign="right">
      <physics noresponse="true"/>
    </orientate>
    <event state="IDLE"  label="Edit" xalign="center" yalign="middle"/>
  </gizmo>

  <gizmo name="SFXEditEditUndo" parent="SFXEditEdit" ref="previous" description="Undo|Undo All|Undo History">
      <orientate ref="previous" width="96" height="24" yalign="bottom">
        <physics noresponse="true"/>
      </orientate>
      <event state="IDLE"  label="Undo" x="24" xalign="left" yalign="middle"/>
    </gizmo>

  <gizmo name="SFXEditEditRedo" parent="SFXEditEdit" ref="previous" description="Redo">
      <orientate ref="previous" width="96" height="24" yalign="bottom">
