 <gizmo name="SFXEditBlank" parent="SFXEdit" description="Blank">
    <orientate ref="SFXEditHelp" width="darklua: xmlAttributeValue = dbScreenWidthGet() - (64 * 6) -1" height="24" xalign="right">
      <physics noresponse="true"/>
    </orientate>
    <event state="IDLE" theme="waterbox"/>
  </gizmo>
 
</s3ge>
