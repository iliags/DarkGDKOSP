    </gizmo>

    <gizmo name="UIEditTransformer2DMoverZ" parent="UIEditTransformer2DBase" ref="previous" description="TopRight`">
      <orientate ref="previous" x="darklua: xmlAttributeValue = uiGizmoPropertyGet(uiGizmoGet('UIEditTransformer2DBase'),'BoundX',0) + 88" y="darklua: xmlAttributeValue = uiGizmoPropertyGet(uiGizmoGet('UIEditTransformer2DBase'),'BoundY',0) - 8" width="16" height="16">
        <physics noresponse="true"/>
