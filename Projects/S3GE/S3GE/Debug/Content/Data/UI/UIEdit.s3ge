  <!-- Transformer Tool -->

  <gadget name="UIEditTransformer2D" description="2D Transformer Tool">

    <gizmo name="UIEditTransformer2DBase" parent="UIEdit" description="Gizmo Transformer">
      <variable name="toggle" value="0"/>
      <orientate x="darklua: xmlAttributeValue = dbScreenWidthGet()/2 - 48" y="darklua: xmlAttributeValue = dbScreenHeightGet()/2 - 48" width="96" height="96">
        <physics noresponse="true"/>
      </orientate>
      <event state="IDLE" behavior="menubox" transition="ghostly" theme="cyanbox" style="standard" scheme="pointingFX" caption="Transformer" xalign="center" yalign="middle"/>
      <event state="ENTER"/>
      <event state="HOVER" scheme="pointingFX"/>
      <event state="DOWN"/>
      <event state="HOLD"/>
      <event state="DRAG"/>
      <event state="UP"/>
      <event state="EXIT"/>
    </gizmo>

    <gizmo name="UIEditTransformer2DMover" parent="UIEditTransformer2DBase" description="TopLeft">
      <variable name="toggle" value="0"/>
      <orientate ref="previous" x="darklua: xmlAttributeValue = uiGizmoPropertyGet(uiGizmoGet('UIEditTransformer2DBase'),'BoundX',0) - 8" y="darklua: xmlAttributeValue = uiGizmoPropertyGet(uiGizmoGet('UIEditTransformer2DBase'),'BoundY',0) - 8" width="16" height="16">
        <physics noresponse="true"/>
      </orientate>
      <event state="IDLE" behavior="pushbox" transition="ghostly" theme="confetti" style="standard" scheme="pointingFX"/>
      <event state="ENTER"/>
      <event state="HOVER"/>
      <event state="DOWN"/>
      <event state="HOLD"/>
      <event state="DRAG"/>
      <event state="UP"/>
      <event state="EXIT"/>
    </gizmo>

    <gizmo name="UIEditTransformer2DMoverX" parent="UIEditTransformer2DBase" ref="previous" description="Left">
      <orientate ref="previous" x="darklua: xmlAttributeValue = uiGizmoPropertyGet(uiGizmoGet('UIEditTransformer2DBase'),'BoundX',0) - 8" y="darklua: xmlAttributeValue = uiGizmoPropertyGet(uiGizmoGet('UIEditTransformer2DBase'),'BoundY',0) + 40" width="16" height="16">
        <physics noresponse="true"/>
      </orientate>
      <event state="IDLE"/>
    </gizmo>

    <gizmo name="UIEditTransformer2DMoverY" parent="UIEditTransformer2DBase" ref="previous" description="Top">
      <orientate ref="previous" x="darklua: xmlAttributeValue = uiGizmoPropertyGet(uiGizmoGet('UIEditTransformer2DBase'),'BoundX',0) + 40" y="darklua: xmlAttributeValue = uiGizmoPropertyGet(uiGizmoGet('UIEditTransformer2DBase'),'BoundY',0) - 8" width="16" height="16">
