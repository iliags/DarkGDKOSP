<s3ge version="1.0">
  
  <!-- GUI (Window-ish Pre-Fabs for MAUI) by Techlord - Contains MAUI Definitions for BASIC Common GUI functionality.  -->
  
  <script language="darklua" type="1"><![CDATA[ uiOutput("Loading GUI Prefab Components...")	]]></script>
  
  <!-- STATES -->
  <state name="ENTER"/>
  <state name="HOVER"/>
  <state name="DOWN"/>
  <state name="HOLD"/>
  <state name="DRAG"/>
  <state name="UP"/>
  <state name="EXIT"/>
  <state name="WORK"/>
  <state name="SCROLLUP"/>
  <state name="SCROLLDOWN"/>
  <state name="SCROLLLOCKUP"/>
  <state name="SCROLLLOCKDOWN"/>
  <state name="MOVE"/>
  <state name="LOAD"/>
  <state name="UNLOAD"/>
  <state name="HIDE"/>
  <state name="SHOW"/>
  <state name="SELECT"/>
  <state name="UNSELECT"/>
  
	<!-- GADGET-LIKE BEHAVIORS -->
	
	<behavior name="pushbox" description="Pre-Fab Button">
		<event state="IDLE">
			<script language="darklua" type="1">
				<![CDATA[
				if uiPointerBound == 1 then
					uiGizmoNextEventState = ENTER
				else
					uiGizmoNextEventState = IDLE
				end
				]]>
			</script>
		</event>
		<event state="ENTER">
			<script language="darklua" type="1">
				<![CDATA[
				if uiPointerBound == 1 then
					uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
					uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
					if uigizmoeventstatecounter > 3 then
						uiGizmoPropertySet(this,'EventStateCounter',0)
						uiGizmoNextEventState = HOVER
					else
						uiGizmoNextEventState = ENTER
					end
				else
					uiGizmoNextEventState = EXIT
				end
				]]>
			</script>
		</event>
		<event state="HOVER">
			<script language="darklua" type="1">
				<![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then 
						uiGizmoNextEventState = DOWN
					else
            uiOutput(uiGizmoPropertyGet(this,'Description'))
						uiGizmoNextEventState = HOVER
					end
				else
					uiGizmoNextEventState = EXIT
				end
				]]>
			</script>
		</event>
		<event state="DOWN">
			<script language="darklua" type="1">
				<![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 8 then
							uiGizmoPropertySet(this,'EventStateCounter',0)
							uiGizmoNextEventState = HOLD
						else
							uiGizmoNextEventState = DOWN
						end
					end	
				else
					uiGizmoNextEventState = EXIT
				end
				]]>
			</script>
		</event>
		<event state="HOLD">
			<script language="darklua" type="1">
				<![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uiGizmoNextEventState = HOLD
					else
						uiGizmoNextEventState = UP
					end
				else
					if uiPointerContact == 1 then 
						uiGizmoNextEventState = DRAG
					end
				end
				]]>
			</script>
		</event>
		<event state="DRAG">
			<script language="darklua" type="1">
				<![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uiGizmoNextEventState = HOLD
					else
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 4 then
							uiGizmoNextEventState = UP
						end
					end
				else
					if uiPointerContact == 1 then 
						uiGizmoNextEventState = DRAG
					else
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 4 then
							uiGizmoNextEventState = UP
						end					
					end
				end
				]]>
			</script>
		</event>
		<event state="UP">
			<script language="darklua" type="1">
				<![CDATA[
				uiGizmoPropertySet(this,'EventStateCounter',0)
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uiGizmoNextEventState = DOWN
					else
						uiGizmoNextEventState = HOVER
					end
				else
					uiGizmoNextEventState = DRAG
				end
				]]>
			</script>
		</event>
		<event state="EXIT">
			<script language="darklua" type="1">
				<![CDATA[
				uiGizmoPropertySet(this,'EventStateCounter',0)
				uiGizmoNextEventState = IDLE
				]]>
			</script>
		</event>
	</behavior>
  
  <behavior name="menubox" description="Pre-Fab Menu">
    <event state="IDLE">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					uiGizmoNextEventState = ENTER
				else
					uiGizmoNextEventState = IDLE
				end
				]]>
      </script>
    </event>
    <event state="ENTER">
      <script language="darklua" type="1">
        <![CDATA[
        
        msg = uiGizmoMessageCreate()
        uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VALUE, MAUI_MSG_GIZMO_CLOSE, 0)
        uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VARIABLE, 'toggle', '0')        
        uiGizmoMessageSend(msg, uiGizmoPropertyGet(this,'Name'), MAUI_MSG_TO_SIBLINGS, 1)
        uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VALUE, MAUI_MSG_GIZMO_OPEN, 0)
        uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VARIABLE, 'toggle', '1')        
        uiGizmoMessageSend(msg, uiGizmoPropertyGet(this,'Name'), MAUI_MSG_TO_SIBLINGS, 0)
        uiGizmoMessageDestroy(msg)
        
				if uiPointerBound == 1 then
					uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
					uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
					if uigizmoeventstatecounter > 3 then
						uiGizmoPropertySet(this,'EventStateCounter',0)
						uiGizmoNextEventState = HOVER
					else
						uiGizmoNextEventState = ENTER
					end
				else
					uiGizmoNextEventState = EXIT
				end
				]]>
      </script>
    </event>
    <event state="HOVER">
      <script language="darklua" type="1">
        <![CDATA[
        
        if uiGizmoVariableGet(this, 'toggle') == '1' then
          msg = uiGizmoMessageCreate()
          uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VALUE, MAUI_MSG_GIZMO_OPEN, 0)
          uiGizmoMessageSend(msg, uiGizmoPropertyGet(this,'Name'), MAUI_MSG_TO_CHILDREN, 0)
          uiGizmoMessageDestroy(msg)        
        end
        
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then 
						uiGizmoNextEventState = DOWN
					else
            uiOutput(uiGizmoPropertyGet(this,'Description'))
						uiGizmoNextEventState = HOVER
					end
				else
					uiGizmoNextEventState = EXIT
				end
				]]>
      </script>
    </event>
    <event state="DOWN">
      <script language="darklua" type="1">
        <![CDATA[
        uiGizmoVariableSet(this, 'toggle', '1')
        msg = uiGizmoMessageCreate()
        uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VALUE, MAUI_MSG_GIZMO_OPEN, 0);
        uiGizmoMessageSend(msg, uiGizmoPropertyGet(this,'Name'), MAUI_MSG_TO_CHILDREN, 0)
        uiGizmoMessageDestroy(msg)       
        
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 8 then
							uiGizmoPropertySet(this,'EventStateCounter',0)
							uiGizmoNextEventState = HOLD
						else
							uiGizmoNextEventState = DOWN
						end
					end	
				else
					uiGizmoNextEventState = EXIT
				end
				]]>
      </script>
    </event>
    <event state="HOLD">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uiGizmoNextEventState = HOLD
					else
						uiGizmoNextEventState = UP
					end
				else
					if uiPointerContact == 1 then 
						uiGizmoNextEventState = DRAG
					end
				end
				]]>
      </script>
    </event>
    <event state="DRAG">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uiGizmoNextEventState = HOLD
					else
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 4 then
							uiGizmoNextEventState = UP
						end
					end
				else
					if uiPointerContact == 1 then 
						uiGizmoNextEventState = DRAG
					else
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 4 then
							uiGizmoNextEventState = UP
						end					
					end
				end
				]]>
      </script>
    </event>
    <event state="UP">
      <script language="darklua" type="1">
        <![CDATA[
        
        uiGizmoToggle = 1 - uiGizmoVariableGet(this, 'toggle')
        uiGizmoVariableSet(this, 'toggle', uiGizmoToggle)
        msg = uiGizmoMessageCreate()
        uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VALUE, MAUI_MSG_GIZMO_CLOSE + uiGizmoToggle, 0);
        uiGizmoMessageSend(msg, uiGizmoPropertyGet(this,'Name'), MAUI_MSG_TO_CHILDREN, 0)
        uiGizmoMessageDestroy(msg)
          
				uiGizmoPropertySet(this,'EventStateCounter',0)
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uiGizmoNextEventState = DOWN
					else
						uiGizmoNextEventState = HOVER
					end
				else
					uiGizmoNextEventState = DRAG
				end
				]]>
      </script>
    </event>
    <event state="EXIT">
      <script language="darklua" type="1">
        <![CDATA[
				uiGizmoPropertySet(this,'EventStateCounter',0)
				uiGizmoNextEventState = IDLE
				]]>
      </script>
    </event>
  </behavior>
  
  <behavior name="checkbox" description="Pre-Fab Checkbox" ref="pushbox">
    <event state="HOVER">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then 
						uiGizmoNextEventState = DOWN
					else
						uiGizmoNextEventState = HOVER
					end
				else
					uiGizmoNextEventState = EXIT
				end
				]]>
      </script>
    </event>    
    
    <event state="DOWN">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
            uiOutput('Check Box Down')
        		uiGizmoNextEventState = DOWN
					else
						uiGizmoNextEventState = UP
					end
				else
					if uiPointerContact == 1 then 
						uiGizmoNextEventState = MOVE
					end
				end
				]]>
      </script>
    </event>
    <event state="HOLD">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 and uiPointerContact == 1 then
						uiGizmoNextEventState = DOWN
				else
					uiGizmoNextEventState = HOLD
				end
				]]>
      </script>
    </event>    
    <event state="UP">
      <script language="darklua" type="1">
        <![CDATA[
				uiGizmoPropertySet(this,'EventStateCounter',0)
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uiGizmoNextEventState = DOWN
					else
            uigizmochecked = 1 - uiGizmoVariableGet(this, 'checked')
            uiGizmoVariableSet(this, 'checked', uigizmochecked)
            uiOutput('Check Box ['..uigizmochecked..']')
						if uigizmochecked == 1 then 
              uiGizmoNextEventState = HOLD
            else
              uiGizmoNextEventState = HOVER
            end
					end
				else
					uiGizmoNextEventState = MOVE
				end
				]]>
      </script>
    </event>
  </behavior>
  
  <behavior name="radiobox" description="Pre-Fab Radio" ref="checkbox">
    <event state="UP">
      <script language="darklua" type="1">
        <![CDATA[
				uiGizmoPropertySet(this,'EventStateCounter',0)
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uiGizmoNextEventState = DOWN
					else
            uigizmochecked = 1 - uiGizmoVariableGet(this, 'checked')
            uiGizmoVariableSet(this, 'checked', uigizmochecked)
            uiOutput('Check Box ['..uigizmochecked..']')
						if uigizmochecked == 1 then 
              uiGizmoNextEventState = HOLD
              msg = uiGizmoMessageCreate()
              uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VARIABLE, 'checked', '0')
              uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_PROPERTY, 'EventState', EXIT)
              uiGizmoMessageSend(msg, uiGizmoPropertyGet(this,'Name'), MAUI_MSG_TO_SIBLINGS, 0)
              uiGizmoMessageDestroy(msg)
            else
              uiGizmoNextEventState = HOVER
            end
					end
				else
					uiGizmoNextEventState = MOVE
				end
				]]>
      </script>
    </event>    
  </behavior>
  
  <behavior name="movebox" ref="pushbox" description="Pre-Fab Mover">
    <event state="DOWN">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 1 then
							uiGizmoPropertySet(this,'EventStateCounter',0)
              
              msg = uiGizmoMessageCreate()
              uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_SCRIPT, 'moverbox_DOWN', "uiGizmoVariableSet(this,'pointeroffsetx', uiGizmoPropertyGet(this,'BoundX') - uiGizmoPropertyGet(uiGizmoGet('point'),'BoundX') );uiGizmoVariableSet(this,'pointeroffsety', uiGizmoPropertyGet(this,'BoundY') - uiGizmoPropertyGet(uiGizmoGet('point'),'BoundY') );uiGizmoVariableSet(this,'pointeroffsetz', uiGizmoPropertyGet(this,'BoundZ') - uiGizmoPropertyGet(uiGizmoGet('point'),'BoundZ') )")
              uiGizmoMessageSend(msg, uiGizmoPropertyGet(this,'Name'), MAUI_MSG_TO_SELF, 1)
              uiGizmoMessageDestroy(msg)
            
							uiGizmoNextEventState = HOLD
						else
							uiGizmoNextEventState = DOWN
						end
					end	
				else
					uiGizmoNextEventState = EXIT
				end
				]]>
      </script>
    </event>   
    <event state="HOLD">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
          
            msg = uiGizmoMessageCreate()
            uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_SCRIPT, 'moverbox_HOLD', "if uiGizmoVariableGet(this, 'pointerlockx') ~= '1' then uiGizmoPropertySet(this, 'BoundX', uiGizmoPropertyGet(uiGizmoGet('point'),'BoundX') + uiGizmoVariableGet(this,'pointeroffsetx') ) end;if uiGizmoVariableGet(this, 'pointerlocky') ~= '1' then uiGizmoPropertySet(this, 'BoundY', uiGizmoPropertyGet(uiGizmoGet('point'),'BoundY') + uiGizmoVariableGet(this,'pointeroffsety') ) end;if uiGizmoVariableGet(this, 'pointerlockz') ~= '1' then uiGizmoPropertySet(this, 'BoundZ', uiGizmoPropertyGet(uiGizmoGet('point'),'BoundZ') + uiGizmoVariableGet(this,'pointeroffsetz') ) end;uiGizmoPropertySet(this,'BoundOrientation',1)" )
            uiGizmoMessageSend(msg, uiGizmoPropertyGet(this,'Name'), MAUI_MSG_TO_SELF, 1)
            uiGizmoMessageDestroy(msg)
        
						uiGizmoNextEventState = HOLD
					else
						uiGizmoNextEventState = UP
					end
				else
					if uiPointerContact == 1 then 
						uiGizmoNextEventState = DRAG
					end
				end
				]]>
      </script>
    </event>
    <event state="DRAG">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uiGizmoNextEventState = HOLD
					else
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 4 then
							uiGizmoNextEventState = UP
						end
					end
				else
					if uiPointerContact == 1 then
          
            msg = uiGizmoMessageCreate()
            uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_SCRIPT, 'moverbox_HOLD', "if uiGizmoVariableGet(this, 'pointerlockx') ~= '1' then uiGizmoPropertySet(this, 'BoundX', uiGizmoPropertyGet(uiGizmoGet('point'),'BoundX') + uiGizmoVariableGet(this,'pointeroffsetx') ) end;if uiGizmoVariableGet(this, 'pointerlocky') ~= '1' then uiGizmoPropertySet(this, 'BoundY', uiGizmoPropertyGet(uiGizmoGet('point'),'BoundY') + uiGizmoVariableGet(this,'pointeroffsety') ) end;if uiGizmoVariableGet(this, 'pointerlockz') ~= '1' then uiGizmoPropertySet(this, 'BoundZ', uiGizmoPropertyGet(uiGizmoGet('point'),'BoundZ') + uiGizmoVariableGet(this,'pointeroffsetz') ) end;uiGizmoPropertySet(this,'BoundOrientation',1)" )
            uiGizmoMessageSend(msg, uiGizmoPropertyGet(this,'Name'), MAUI_MSG_TO_SELF, 1)
            uiGizmoMessageDestroy(msg)
            
						uiGizmoNextEventState = DRAG
					else
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 4 then
							uiGizmoNextEventState = UP
						end					
					end
				end
				]]>
      </script>
    </event>
  </behavior>
  
  <behavior name="textbox" description="Pre-Fab Textbox">
    <event state="IDLE">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
          msg = uiGizmoMessageCreate()
          uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VALUE, MAUI_MSG_GIZMO_OPEN, 0)
          uiGizmoMessageSend(msg, 'caret', MAUI_MSG_TO_SELF, 1)
          uiGizmoMessageDestroy(msg)
 
          msg = uiGizmoMessageCreate()
          uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VALUE, MAUI_MSG_GIZMO_COLLISION_ENABLE, 0)
          uiGizmoMessageSend(msg, 'textboxkeyboard', MAUI_MSG_TO_CHILDREN, 1)
          uiGizmoMessageDestroy(msg)
          
          dbEntryBufferClear()
          
					uiGizmoNextEventState = ENTER
          
				else
        
          if uiGizmoVariableGet(this,'capturedonstart') ~= '1' then   
            uiGizmoVariableSet(this,'capturedonstart', '1')
            uiGizmoLinesCapture(this,IDLE)
          end
          
					uiGizmoNextEventState = IDLE
				
        end
				]]>
      </script>
    </event>
    <event state="ENTER">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
       
					uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
					uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
					if uigizmoeventstatecounter > 3 then
						uiGizmoPropertySet(this,'EventStateCounter',0)
            uiGizmoLineDisplay(this,255,255,255)
						uiGizmoNextEventState = HOVER
					else
            uiGizmoLineDisplay(this,191,191,191)          
						uiGizmoNextEventState = ENTER
					end
				else
          uiGizmoLineDisplay(this,255,0,0)
					uiGizmoNextEventState = EXIT
				end
				]]>
      </script>
    </event>
    <event state="HOVER">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
            uiGizmoLineDisplay(this,255,255,255)
						uiGizmoNextEventState = DOWN
					else
            uiOutput(uiGizmoPropertyGet(this,'Description'))
            uiGizmoLineDisplay(this,255,255,255)
					  uiCaretLocate(this,0)          
						uiGizmoNextEventState = HOVER
					end
				else
          uiGizmoLineDisplay(this,255,0,0)
					uiGizmoNextEventState = EXIT
				end
				]]>
      </script>
    </event>
    <event state="DOWN">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 16 then
							uiGizmoPropertySet(this,'EventStateCounter',0)
              uiGizmoLineDisplay(this,255,255,255)
							uiGizmoNextEventState = HOLD
						else
              uiCaretLocate(this,1)
              uiGizmoLineDisplay(this,255,255,255)
							uiGizmoNextEventState = DOWN
						end
					end	
				else
          uiGizmoLineDisplay(this,255,0,0)
					uiGizmoNextEventState = EXIT
				end
				]]>
      </script>
    </event>
    <event state="HOLD">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
            uiCaretLocate(this,2)
            uiGizmoLineDisplay(this,255,255,255)
 						uiGizmoNextEventState = HOLD
					else
          uiGizmoLineDisplay(this,255,255,255)
						uiGizmoNextEventState = UP
					end
				else
					if uiPointerContact == 1 then
            uiGizmoLineDisplay(this,255,255,0)
						uiGizmoNextEventState = DRAG
					end
				end
				]]>
      </script>
    </event>
    <event state="DRAG">
      <script language="darklua" type="1">
        <![CDATA[
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
            uiGizmoLineDisplay(this,255,255,0)
						uiGizmoNextEventState = HOLD
					else
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 10 then
              uiGizmoLineDisplay(this,255,255,255)
							uiGizmoNextEventState = UP
						end
					end
				else
					if uiPointerContact == 1 then
            uiGizmoLineDisplay(this,255,255,0)
            uiGizmoNextEventState = DRAG
					else
						uigizmoeventstatecounter = uiGizmoPropertyGet(this,'EventStateCounter') + 1
						uiGizmoPropertySet(this,'EventStateCounter',uigizmoeventstatecounter)
						if uigizmoeventstatecounter == 10 then
              uiGizmoLineDisplay(this,255,255,255)
							uiGizmoNextEventState = UP
						end					
					end
				end
				]]>
      </script>
    </event>
    <event state="UP">
      <script language="darklua" type="1">
        <![CDATA[
				uiGizmoPropertySet(this,'EventStateCounter',0)
				if uiPointerBound == 1 then
					if uiPointerContact == 1 then
            uiGizmoLineDisplay(this,255,255,255)
						uiGizmoNextEventState = DOWN
					else
            uiCaretLocate(this,2)
            uiGizmoLineSelectionEdit(this,1)
            uiGizmoLineDisplay(this,255,255,255)
						uiGizmoNextEventState = HOVER
					end
				else
            uiGizmoLineDisplay(this,255,255,0)
        		uiGizmoNextEventState = DRAG
				end
				]]>
      </script>
    </event>
    <event state="EXIT">
      <script language="darklua" type="1">
        <![CDATA[
        
        msg = uiGizmoMessageCreate()
        uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VALUE, MAUI_MSG_GIZMO_CLOSE, 0)
        uiGizmoMessageSend(msg, 'caret', MAUI_MSG_TO_SELF, 1)
        uiGizmoMessageDestroy(msg)
        
        msg = uiGizmoMessageCreate()
        uiGizmoMessageInsert(msg, MAUI_MSG_TYPE_VALUE, MAUI_MSG_GIZMO_COLLISION_DISABLE, 0)
        uiGizmoMessageSend(msg, 'textboxkeyboard', MAUI_MSG_TO_CHILDREN, 1)
        uiGizmoMessageDestroy(msg)        
          
        uiGizmoLinesCapture(this,IDLE)
        
				uiGizmoPropertySet(this,'EventStateCounter',0)
				uiGizmoNextEventState = IDLE
				]]>
      </script>
    </event>
