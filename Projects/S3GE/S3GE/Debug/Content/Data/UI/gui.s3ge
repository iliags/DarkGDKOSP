          if uiGizmoSpriteY < uiGizmoBoundY then
   					if uiGizmoSpriteObjectAlpha < 255 then dbSpriteAlphaSet(uiGizmoSpriteObject, uiGizmoSpriteObjectAlpha + 15) end         
            uiGizmoSpriteY = uiGizmoSpriteY + 15
            uiGizmoVariableSet(this,'spritey',uiGizmoSpriteY)
