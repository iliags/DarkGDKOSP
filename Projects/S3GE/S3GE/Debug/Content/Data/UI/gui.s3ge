  <!-- TRANSITIONS -->
  
  <transition name="ghostly" description="Sprite Display Fade In & Out">
    <event state="IDLE">
      <script language="darklua" type="1">
        <![CDATA[
				 if uiGizmoPreviousEventState ~= IDLE then
				 	uiGizmoSpriteObject = uiGizmoPropertyGet(this,'SpriteObject')
					uiGizmoSpriteObjectAlpha = dbSpriteAlphaGet(uiGizmoSpriteObject)
					if uiGizmoSpriteObjectAlpha > 150 then
						dbSpriteAlphaSet(uiGizmoSpriteObject, uiGizmoSpriteObjectAlpha - 5)
					else
						dbSpriteAlphaSet(uiGizmoSpriteObject, 150)
						uiGizmoPreviousEventState = IDLE
					end
				 end
				 ]]>
      </script>
    </event>
    <event state="ENTER"/>
    <event state="HOVER">
      <script language="darklua" type="1">
        <![CDATA[
				 if uiGizmoPreviousEventState == ENTER then
				 	uiGizmoSpriteObject = uiGizmoPropertyGet(this,'SpriteObject')
					uiGizmoSpriteObjectAlpha = dbSpriteAlphaGet(uiGizmoSpriteObject)
					if uiGizmoSpriteObjectAlpha < 255 then
						dbSpriteAlphaSet(uiGizmoSpriteObject, uiGizmoSpriteObjectAlpha + 15)
					else
						dbSpriteAlphaSet(uiGizmoSpriteObject, 255)
						uiGizmoPreviousEventState = HOVER
					end
				 end
				 ]]>
      </script>
    </event>
    <event state="DOWN"/>
    <event state="HOLD"/>
    <event state="DRAG"/>
    <event state="UP"/>
    <event state="EXIT"/>
  </transition>
  
  <transition name="xshift" description="Move Along X from left to position">
    
		<event state="IDLE">
			<script language="darklua" type="1">
				<![CDATA[
         --hide gizmo; Current Bound X set by controller physics        
				 if uiGizmoPreviousEventState ~= IDLE then
			 	  uiGizmoSpriteObject = uiGizmoPropertyGet(this,'SpriteObject')
				  uiGizmoSpriteObjectAlpha = dbSpriteAlphaGet(uiGizmoSpriteObject)
          uiGizmoSpriteX = uiGizmoVariableGet(this,'spritex') + 0
					if uiGizmoSpriteX > 0 then
					  if uiGizmoSpriteObjectAlpha > 0 then dbSpriteAlphaSet(uiGizmoSpriteObject, uiGizmoSpriteObjectAlpha - 5) end 
            uiGizmoSpriteX = uiGizmoSpriteX - 1
            uiGizmoVariableSet(this,'spritex',uiGizmoSpriteX)
            uiGizmoPropertySet(this,'BoundX',uiGizmoSpriteX)
          else
            dbSpriteAlphaSet(uiGizmoSpriteObject, 0)
            uiGizmoVariableSet(this,'spritex',0)
        		uiGizmoPreviousEventState = IDLE
					end
				 end
				 ]]>
			</script>
		</event>
    <event state="ENTER"/>
		
		<event state="HOVER">
			<script language="darklua" type="1">
				<![CDATA[
				 if uiGizmoPreviousEventState == ENTER then
				 	uiGizmoSpriteObject = uiGizmoPropertyGet(this,'SpriteObject')
					uiGizmoSpriteObjectAlpha = dbSpriteAlphaGet(uiGizmoSpriteObject)
          uiGizmoSpriteX = uiGizmoVariableGet(this,'spritex')
          uiGizmoBoundX = uiGizmoPropertyGet(this,'BoundX')
          if uiGizmoSpriteX < uiGizmoBoundX then
   					if uiGizmoSpriteObjectAlpha < 255 then dbSpriteAlphaSet(uiGizmoSpriteObject, uiGizmoSpriteObjectAlpha + 5) end         
            uiGizmoSpriteX = uiGizmoSpriteX + 15
            uiGizmoVariableSet(this,'spritex',uiGizmoSpriteX)
            uiGizmoPropertySet(this,'BoundX',uiGizmoSpriteX)
          else
 						dbSpriteAlphaSet(uiGizmoSpriteObject, 255)         
            uiGizmoVariableSet(this,'spritex',uiGizmoBoundX)
        		uiGizmoPreviousEventState = HOVER
					end
				 end
				 ]]>
			</script>
		</event>
    <event state="DOWN"/>
		<event state="HOLD"/>
		<event state="DRAG"/>
		<event state="UP"/>
		<event state="EXIT"/>
	</transition>
  
  <transition name="yshift" description="Move Along Y from top to position">
    <event state="IDLE">
      <script language="darklua" type="1">
        <![CDATA[
         --hide gizmo; Current Bound Y set by controller physics        
