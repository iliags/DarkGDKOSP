            dbSpriteAlphaSet(uiGizmoSpriteObject, 0)
            uiGizmoVariableSet(this,'spritey',0)
        		uiGizmoPreviousEventState = IDLE
					end
				 end
				 ]]>
      </script>
    </event>
    <event state="ENTER"/>
